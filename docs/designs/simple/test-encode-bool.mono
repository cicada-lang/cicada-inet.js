drop : { (- A) }
drop = {
  [x : A]
}

swap : { (- A) (- A) A A }
swap = {
  [x : A]
  [y : A]
  x y
}

id : { (- A) A }
id = {
  [x : A]
  x
}

// type Boolean = { (- A) (- A) A }

// true : { Boolean }
true : { (- A) (- A) A }
true = {
  [x : A]
  [y : A]
  x
}

// false : { Boolean }
false : { (- A) (- A) A }
false = {
  [x : A]
  [y : A]
  y
}

// not : { (- Boolean) Boolean }
not : { (- { (- A) (- A) A }) (- A) (- A) A }
not = {
  [a : { (- A) (- A) A }]
  [x : A]
  [y : A]
  x y a exe
}

true_not : { (- A) (- A) A }
true_not = {
  { true } not
}

true_not_not : { (- A) (- A) A }
true_not_not = {
  { { true } not } not
}


