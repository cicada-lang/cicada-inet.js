(define-cons sole 0)

(define-cons null 0)
(define-cons cons 2)

(define-cons diff 2)

(define-elim diff-open 2)
(define-rule (diff diff-open) (rot rot connect))

(define-elim diff-append 2)
(define-rule (diff diff-append) (rot rot swap diff-open swap diff))

(define-net diff-append-example-1
  wire sole cons diff
  wire sole cons sole cons diff
  diff-append)

(define-net diff-append-example-2
  wire sole cons sole cons diff
  wire sole cons sole cons diff
  diff-append)
