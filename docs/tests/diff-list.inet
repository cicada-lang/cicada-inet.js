(define-node sole (-> [] [Trivial *]))

(define-node null
  (-> []
      [TrivialList *]))

(define-node cons
  (-> [TrivialList Trivial]
      [Nat *]))

(define-node append
  (-> [TrivialList TrivialList *]
      [TrivialList]))

(define-rule
  [null append]
  [])

(define-rule
  [cons append]
  [rot rot append swap cons])

(define-node diff
  (-> [TrivialList TrivialList]
      [TrivialDiffList *]))

(define-node diff-append
  (-> [TrivialDiffList TrivialDiffList *]
      [TrivialDiffList]))

(define-node diff-open
  (-> [TrivialDiffList TrivialDiffList *]
      [TrivialDiffList]))

(define-rule
  [diff diff-append]
  [rot rot swap diff-open swap diff])

(define-rule
  [diff diff-open]
  [rot rot connect])

(define-net diff-append-example-1 (-> [] [TrivialDiffList])
  wire sole cons diff
  wire sole cons sole cons diff
  diff-append)

(define-net diff-append-example-2 (-> [] [TrivialDiffList])
  wire sole cons sole cons diff
  wire sole cons sole cons diff
  diff-append)
