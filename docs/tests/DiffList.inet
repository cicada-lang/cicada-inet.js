defnode sole -- value! end

defnode null -- value! end

defnode cons
  head
  tail
  -------
  value!
end

defnode diff
  left
  right
  ------
  value!
end

defnode diff_append
  target!
  rest
  ------
  return
end

defrule diff diff_append
  (diff)-left (diff_append)-rest diff_open
  (diff)-right diff
  return-(diff_append)
end

defnode diff_open
  target!
  list
  ------
  return
end

defrule diff diff_open
  (diff_open)-list (diff)-left connect
  (diff)-right
  return-(diff_open)
end

defnet one_two_soles
  wire sole cons diff
  wire sole cons sole cons diff
  diff_append
end

defnet two_two_soles
  wire sole cons sole cons diff
  wire sole cons sole cons diff
  diff_append
end
