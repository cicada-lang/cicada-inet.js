defnode sole
  ------
  value!
end

defnode null
  ------
  value!
end

defnode cons
  head
  tail
  -------
  value!
end

defnode append
  target!
  rest
  ------
  return
end

defrule null append
  (append)-rest
  return-(append)
end

defrule cons append
  (cons)-tail
  (append)-rest
  append
  (cons)-head cons
  return-(append)
end

defnet six_soles
  null sole cons sole cons sole cons
  null sole cons sole cons sole cons
  append
end

show six_soles end
run six_soles end