{
  "name": "@cicada-lang/inet",
  "version": "0.0.11",
  "repository": "github:cicada-lang/inet",
  "engines": {
    "node": "< 18"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test:js": "test-runner test node 'lib/**/*.test.js'",
    "test:render": "test-runner test './bin/inet.js render' 'docs/tests/**/*.inet'",
    "test": "npm-run-all test:*",
    "format": "prettier src docs --write"
  },
  "bin": {
    "inet": "bin/inet.js"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/nanoid": "^2.1.0",
    "@types/node": "^17.0.34",
    "@xieyuheng/test-runner": "^0.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@cicada-lang/sexp": "^0.0.24",
    "@hpcc-js/wasm": "^1.14.1",
    "@xieyuheng/command-line": "^0.0.7",
    "@xieyuheng/ty": "^0.1.12",
    "axios": "^0.27.2",
    "fast-deep-equal": "^3.1.3",
    "nanoid": "^3.3.4",
    "node-watch": "^0.7.3",
    "picocolors": "^1.0.0",
    "readdirp": "^3.6.0"
  },
  "license": "GPL-3.0-or-later"
}
